window.top.ziCampaign=window.top.ziCampaign||[];
function getCampaign(d,c){var a=0,b=[],e=location.protocol+"//"+location.hostname+location.pathname+location.search;try{for(;1>window.top.ziCampaign.length&&3>a;)window.top.ziCampaign=getKeyword(e),a++;"undefined"!==typeof window.top.ziCampaign.keyword&&window.top.ziCampaign.keyword.forEach(function(a){a.ad_unit_code===c&&b.push(a.campaigns[0].code)});"undefined"!==typeof window.top.ziCampaign.age_gender&&window.top.ziCampaign.age_gender.forEach(function(a){a.ad_unit_code===c&&b.push(a.campaigns[0].code)});
return 0>=b.length?"":b[Math.floor(Math.random()*b.length)]}catch(f){return""}}function getKeyword(d){var c=[];try{var a=new XMLHttpRequest;a.open("GET","https://campaign.breaktime-inc.com/campaign?url="+d,!1);a.onload=function(){var a=JSON.parse(this.response);a.status&&(c=a.data)};a.send()}catch(b){}return c};